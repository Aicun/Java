		int count = countRobotShoots("SCCSSC");
		System.out.println(count);
	}
	
	public static void Calculate(int count, String program) {
		
	}
	
	public static int countRobotShoots(String program)
	{
		char[] cs = program.toCharArray();
		int count = 0;
		int shoot = 1;
		for (char c : cs) {
			if(c == 'C') {
				shoot = shoot *2;
			}
			if (c == 'S') {
				count += shoot;
			}
		}
		return count;
	}
}
