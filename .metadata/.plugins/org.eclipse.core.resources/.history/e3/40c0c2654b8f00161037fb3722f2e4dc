package com.lac.codility;

public class CodilityPrimeComposit {

	public static void main(String[] args) {
		
	}
	
	public static int minPerimeterRectangle(int n){
		int i=1;
		int min = n;
		int perimeter = 0;
		while(i*i<=n){
			if(n % i == 0){
				int other = n/i;
				perimeter = i+other;
				perimeter = Math.min(arg0, arg1)
			}
		}
	}

}
